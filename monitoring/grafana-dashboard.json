  "panels": [
    {
      "type": "stat",
      "title": "Total HTTP Requests",
      "gridPos": { "x": 0, "y": 0, "w": 6, "h": 3 },
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(psych_http_requests_total)",
          "legendFormat": "Requests"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Error Rate (%)",
      "gridPos": { "x": 6, "y": 0, "w": 6, "h": 3 },
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "(sum(psych_http_requests_total{status=~\"5..\"}) / sum(psych_http_requests_total)) * 100",
          "legendFormat": "Error %"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Requests Per Second (RPS)",
      "gridPos": { "x": 0, "y": 3, "w": 12, "h": 5 },
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(psych_http_requests_total[1m])",
          "legendFormat": "{{route}} - {{method}}"
        }
      ],
      "lines": true
    },
    {
      "type": "graph",
      "title": "Latency p50 / p90 / p99",
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 6 },
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(psych_http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.90, sum(rate(psych_http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p90"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(psych_http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p99"
        }
      ]
    },
    {
      "type": "table",
      "title": "Requests by Route / Status",
      "gridPos": { "x": 0, "y": 14, "w": 12, "h": 6 },
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum by (route,method,status)(psych_http_requests_total)"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Node.js Memory Usage",
      "gridPos": { "x": 0, "y": 20, "w": 12, "h": 6 },
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "psych_process_resident_memory_bytes",
          "legendFormat": "Resident Memory"
        },
        {
          "expr": "psych_process_heap_used_bytes",
          "legendFormat": "Heap Used"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Node.js CPU Usage",
      "gridPos": { "x": 0, "y": 26, "w": 12, "h": 6 },
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(process_cpu_seconds_total[1m])",
          "legendFormat": "CPU seconds/sec"
        }
      ]
    },
    {
      "type": "heatmap",
      "title": "Request Duration Heatmap",
      "gridPos": { "x": 0, "y": 32, "w": 12, "h": 8 },
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(psych_http_request_duration_seconds_bucket[5m])",
          "legendFormat": "{{le}}"
        }
      ]
    }
  ],
